{
    "useTabs": false,
    "tabWidth": 4,
    "trailingComma": "none",
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": false,
    "printWidth": 130,
    "bracketSameLine": false,
    "plugins": ["prettier-plugin-svelte"],
	"overrides": [
        { "files": "*.json", "options": { "parser": "json" } },
        { "files": "*.svelte", "options": { "parser": "svelte" } }
    ]
}

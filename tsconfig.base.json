{
  "compilerOptions": {
    "strict": true,
	"strictNullChecks": true,
	"esModuleInterop": true,
	"emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noUnusedLocals": true,
	"skipLibCheck": true,
	"sourceMap": true,
	"moduleResolution": "node",
	"allowJs": true,
	"checkJs": true,	
	"forceConsistentCasingInFileNames": true,
	"resolveJsonModule": true	,
	"lib": [      
      "es2023",
      "dom"
    ]
  }
}
